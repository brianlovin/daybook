{
  "name": "daybook",
  "version": "1.0.0",
  "description": "Keep track of what's important to you.",
  "main": "index.js",
  "repository": "https://github.com/brianlovin/daybook",
  "author": "Brian Lovin",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-plugin-import-inspector": "^2.0.0",
    "babel-plugin-inline-import-graphql-ast": "^2.2.0",
    "babel-plugin-styled-components": "^1.1.7",
    "babel-plugin-syntax-async-generators": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "flow-bin": "^0.63.1",
    "flow-typed": "^2.3.0",
    "now-env": "^3.0.3",
    "pre-commit": "^1.2.2",
    "prettier": "^1.9.2",
    "prettier-eslint": "^8.7.0",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "dev:web": "(cd web && yarn run dev)",
    "dev:api": "cross-env NODE_PATH=./ cross-env NODE_ENV=development cross-env DEBUG=api*,shared:middlewares*,-api:resolvers cross-env DIR=api backpack",
    "dev:mobile": "(cd mobile && yarn start)",
    "dev:mobile:ios": "(cd mobile && yarn run ios)",
    "dev:mobile:android": "cd mobile && yarn run android",
    "prebuild:web": "rimraf build-web",
    "build:web": "cross-env NODE_PATH=./ cross-env DIR=web backpack build",
    "postbuild:web": "cp now.json build-web/now.json",
    "prebuild:api": "rimraf build-api",
    "build:api": "cross-env NODE_PATH=./ cross-env DIR=api backpack build",
    "postbuild:api": "cp api/package.json build-api/package.json && cp now.json build-api/now.json",
    "flow": "flow",
    "precommit": "lint-staged",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote --trailing-comma es5",
      "git add"
    ]
  },
  "dependencies": {
    "backpack-core": "^0.5.0",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "express": "^4.16.2",
    "graphql": "^0.13.1",
    "jsonwebtoken": "^8.1.1",
    "keygrip": "^1.0.2",
    "passport": "^0.4.0",
    "webpack-module-manifest-plugin": "^0.1.0"
  }
}
